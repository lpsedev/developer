#include<stdio.h>
#include<stdlib.h>

int main()
{
   double na, nb, media;

    printf("Digite a primeira nota: ");
    scanf("%.1lf", &na);
    printf("Digite a segunda nota: ");
    scanf("%.1lf", &nb);
    
    media = (na * 3.5)+(nb * 7.5) / 11;
    
    printf("MEDIA = %.5lf \n", media);
    
    return 0;
}
